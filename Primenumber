package selenium;

public class Prime {

	public static void main(String [] args)
	{
		int a=9;
		int temp=0;
		for(int i=2;i<a;i++)
		{
			if(a%i == 0)
			{
				temp=1;
				
			}
			}
		if (temp == 1)
		{
			System.out.println(" Not prime number"+a);
			
		}
		else
		{
			System.out.println("Is a prime number"+a);
		}
	}
}
