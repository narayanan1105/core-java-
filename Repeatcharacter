package selenium;

public class Repeatcharacter {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String a = "engineering"; 
		int count = 0;
		char check ='e';		
		for (int i=0; i<a.length(); i++)
		{
			
			if (a.charAt(i) == check) 
			{
			
		count++;
		
			}
		}
		
		System.out.println("check character " + check + " how maany times " + count );
	}

}
