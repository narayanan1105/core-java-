package selenium;

public class Sample3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
	

		for(int i=1;i<=7;i++)
		{
			for(int j=1;j<=i;j++)
			{
			System.out.print(j+" ");
			
			}

		System.out.println(); 
			}

		int n =6;
		for(int l=1;l<=6;l++){
			
		for(int m=1;m<=n;m++){
			System.out.print(m+" ");                                                                                              
		}
		n=n-1;
		System.out.println();
		
		}
			

	}

}
